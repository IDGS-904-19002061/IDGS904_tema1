@model List<IDGS904_tema1.Models.Productos>

@{
    ViewBag.Title = "MostrarPulques";
}

<h2>MostrarPulques</h2>

<table>
    <thead>
    <th>Nombre</th>
    <th>Descripcion</th>
    <th>Cantidad</th>
    <th>Produccion</th>
    </thead>

    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Nombre</td>
            <td>@item.Descripcion</td>
            <td>@item.Cantidad</td>
            <td>@item.Produccion</td>
        </tr>
    }
</table>

